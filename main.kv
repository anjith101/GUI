#:import Factory kivy.factory.Factory
<FoodButton@Button>:
    size_hint: 0.2,0.2
    font_size: 10
    text_size: self.size     
    halign: 'center'
    valign: 'bottom'
    color: 1, 0, 0, 1
    bold: True
    italic: True
    background_down: ''
    on_press:
        app.call(self)
    on_release:
        Factory.MyPopup().open()

<MyPopup>:
    
    auto_dismiss: False
    size_hint: 0.5,0.6
    title: 'QUANTITY'
    background: 'black.jpg'
    separator_color: 0.4, 0.4, 0.4, 1
    title_align: 'center'
    FloatLayout: 
        name : na 
        size_hint: 1,1
        pos_hint: {"right":1, "top":1}
        rows: 1
        cols: 1
        padding: 10
        spacing: 10


        Knob:
            id: na
            size_hint: 0.4,0.65
            pos_hint: {"right":0.4, "top":0.85}
            min: 0
            max: 15
            step: 10
            value: 0  # Default position of knob.
            knobimg_source: "knob.png"  # Knob texture
            show_label: True  # Show central label
            show_marker: True  # Show surrounding marker
            marker_img: "bline.png" # Marker texture image
            knob_size: 0.9  # Scales knob size to leave space for marker
            markeroff_color: 0, 0, 0, 0
            Label:
                
                text: "{}".format(int(self.parent.value))
                center: self.parent.center
                font_size: 40
                bold: True
                italic: True
                color: 0, 1, .5, 1

        Button:
            id:'lb1'
            size_hint: 0.4,0.3
            pos_hint: {"right":1, "top":0.9}
            font_size: 20
            text: 'ORDER'
            bold: True
            italic: True
            background_normal: 'pb.png'
            background_down: 'pb.png'
            color: 0, 1, 1, 1
            
            on_press:
                app.callback(self)
            on_release:
                root.dismiss()

        Button:
            size_hint: 0.4,0.3
            pos_hint: {"right":1, "top":0.4}
            font_size: 20
            text: 'CANCEL'
            bold: True
            italic: True
            background_normal: 'pb.png'
            background_down: 'pb.png'
            color: 0, 1, 1, 1
        
            on_release:
                root.dismiss()
